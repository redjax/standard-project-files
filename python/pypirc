## ~/.pypirc
#  chmod: 600
[distutils]
index-servers=
    pypi
    testpypi

## Example of a local, private Python package index
# [local]
# repository = http://127.0.0.1:8080
# username = test 
# password = test

[testpypi]
username = __token__ 
password = <pypi-test-token>

[pypi]
repository = https://upload.pypi.org/legacy/
username = __token__
password = <pypi-token>

